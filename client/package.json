  "version": "0.0.1",
  "description": "This Web Application is a real time chat application with end to end encryption",
  "main": "index.jsx",
  "scripts": {
    "webpack:start": "webpack serve --config ./webpack/webpack.config.js --inline --progress --env env=development",
    "webpack:build": "webpack --config ./webpack/webpack.config.js --progress --env env=production",
    "webpack:analyze": "npm run webpack:build -- --env addon=bundleanalyzer",
    "serve:webpack:analyze": "serve -s build/report.html",
    "postcss:scss:watch": "postcss ./src/assets/scss/tailwind/tailwind-input.scss -o ./src/assets/scss/tailwind-output.scss --verbose --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "jest:watchAll": "jest --watchAll",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "!**/node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdirshaddev/yexopis.git"
  },
  "keywords": [],
