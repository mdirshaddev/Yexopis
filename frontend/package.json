  "main": "src/index.tsx",
  "description": "Frontend of this Web App built using TypeScript Webpack based React and Redux",
  "scripts": {
    "start": "export NODE_ENV=development && webpack serve --env env=development --progress --hot",
    "build": "export NODE_ENV=production && webpack --env env=production --progress",
    "stats": "webpack --json > stats.json",
    "lint": "eslint src/**/*.tsx --quiet --fix",
    "postcss": "postcss ./src/static/scss/*.scss --dir ./src/static/css --ext css --verbose",
    "postcss:watch": "TAILWIND_MODE=watch yarn postcss --watch",
    "serve": "serve -s build",
    "serve:report": "serve -s report",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
